{
  "expo": {
    "name": "NeoSynth",
    "slug": "NeoSynth",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/neosynt.png",
    "scheme": "neosynth",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neosynth.app",
      "buildNumber": "1.1.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSAppleMusicUsageDescription": "Esta app reproduce m√∫sica y necesita acceso a los controles de audio."
      }
    },
    "android": {
      "icon": "./assets/images/neosynt.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/neosynt.png",
        "backgroundColor": "#5752D7"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.neosynth.app",
      "versionCode": 4,
      "usesCleartextTraffic": true,
      "permissions": [
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/neosynt.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "15c21669-7085-4e96-8825-91cdb556b400"
      }
    }
  }
}
